        "@nuxtjs/axios": "^5.13.6",
        "@nuxtjs/device": "^2.1.0",
        "@nuxtjs/pwa": "^3.3.5",
        "axios": "^0.27.2",
        "cookie-parser": "^1.4.6",
        "cookie-universal-nuxt": "^2.2.1",
        "core-js": "^3.24.0",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "express-fileupload-validator": "^2.0.1",
        "express-oauth2-jwt-bearer": "^1.1.0",
        "express-rate-limit": "^6.5.1",
        "fast-xml-parser": "^4.0.9",
        "helmet": "^5.1.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.5.0",
        "nodemailer": "^6.7.7",
        "nuxt": "^2.15.8",
        "nuxt-start": "^2.15.8",
        "signale": "^1.4.0",
        "uuid": "^8.3.2",
